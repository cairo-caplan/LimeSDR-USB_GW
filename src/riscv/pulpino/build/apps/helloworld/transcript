add wave -position end sim:/tb/uart/*
add wave -position end  sim:/tb/top_i/core_region_i/axi_mem_addr
add wave -position end  sim:/tb/top_i/core_region_i/axi_mem_we
add wave -position end  sim:/tb/top_i/core_region_i/axi_mem_be
add wave -position end  sim:/tb/top_i/core_region_i/axi_mem_rdata
add wave -position end  sim:/tb/top_i/core_region_i/axi_mem_wdata
add wave -position end  sim:/tb/top_i/core_region_i/axi_instr_req
add wave -position end  sim:/tb/top_i/core_region_i/axi_instr_addr
add wave -position end  sim:/tb/top_i/core_region_i/axi_instr_we
add wave -position end  sim:/tb/top_i/core_region_i/axi_instr_be
add wave -position end  sim:/tb/top_i/core_region_i/axi_instr_rdata
add wave -position end  sim:/tb/top_i/core_region_i/axi_instr_wdata
run -all
